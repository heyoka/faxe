
def v1 =
    |value_emitter()
    .every(3s)
    .type(point)
    .align()

def v2 =
    |value_emitter()
    .every(5s)
    .type(point)
    .align()

v1
    |join(v2)
    .field_merge('val')
    .tolerance(3s)
    .missing_timeout(3s)
    .fill(none)

v1
    %|join(v2)
    %.prefix('v1.joined', 'v2.joined')
    %.tolerance(3s)
    %.missing_timeout(3s)
    %.fill(none)

    |debug()

