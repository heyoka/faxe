def v1 =
    |value_emitter()
    .every(200ms)
    .type(point)

def window =
v1
    |win_time()
    .every(12s)
    .period(12s)

window
    |variance()
    .perc(85)
    .field('val')
    .as('variance')
window
    |sum()
    .as('sum')
    .field('val')

def median =
window
    |median()
    .as('median')
    .field('val')

median
    |first()
    .as('first')
    .field('median')

