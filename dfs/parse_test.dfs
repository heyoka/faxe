 def energy =
    |modbus('hregs')
    .ip('127.0.0.1')
    .port(8899)
    .device(255)
    .every(1s)
    .from(2127)
    .count(5)
    .as('Energy.ActiveEnergyConsumption', 'Energy.MaximalCurrentValue',
        'Energy.BlindEnergyDelivered', 'Energy.Max.A', 'Energy.Min.A')
    .read(3009, 2, 'float32', 'Energy.ActiveEnergyConsumption')
