

 |value_emitter()
 .every(5s)
 .type(point)

    |modbus_multi()
    .ip('127.0.0.1')
    .port(8899)
    .device(255)
    .every(7s)
    .align()
    .function('hregs', 'hregs', 'hregs', 'hregs', 'hregs', 'hregs', 'hregs')
    .from(2701, 2699, 2709, 2707, 2717, 2715, 3009)
    .count(2,2,2,2,2,2,2)
    .as(
        'ActiveEnergyRcvd',
        'ActiveEnergyDelvd',
        'ReactiveEnergyRcvd',
        'ReactiveEnergyDelvd',
        'ApparentEnergyRcvd',
        'ApparentEnergyDelvd',
        'MaximalCurrentValue'
    )
    .output('float32', 'float32', 'float32', 'float32', 'float32', 'float32', 'float32')
    .max_connections(333)

    |debug()




