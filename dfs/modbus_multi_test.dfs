 def energy =
    |modbus_multi('hregs')
    .ip('127.0.0.1')
    .port(8899)
    .device(255)
    .every(1s)

    .function('coils', 'hregs', 'iregs')
    .from(2127, 3008, 104)
    .count(1, 2, 2)
    .as('Energy.ActiveEnergyConsumption', 'Energy.MaximalCurrentValue', 'Energy.BlindEnergyDelivered')
    .output('', 'float32', 'float32')
    .signed(true, true, false)

