def host = '127.0.0.1'
def port = 5432
def sql = <<< SELECT AVG(data_obj['x']['cur']) as avg_x_cur FROM table WHERE id > 33 >>>
def user = 'crate'
def database = 'doc'

|postgresql_query()
.host(host)
.port(port)
.database(database)
.user(user)

.query(sql)
.group_by_time(3m)
.every(5s)
.period(18m)
.align()