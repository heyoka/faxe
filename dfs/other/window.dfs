def v1 =
    |value_emitter()
    .every(1s)
    .type('point')
    .align()

def window =
v1
    |win_time()
    .every(5s)
|debug('info')

window
    |aggregate()
    .fields('val', 'val', 'val', 'val')
    .functions('variance', 'sum', 'avg', 'count_distinct')
    .as('variance', 'sum', 'avg', 'count_distinct')

|debug()


